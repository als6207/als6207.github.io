<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mcnc.persistence.UserInfoMapper">

	<insert id="insertUserInfo" parameterType="java.lang.String">
		INSERT INTO USERINFO 
		   ()
	</insert>

	<select id="selectUserInfo" parameterType="java.lang.String">
		SELECT ID, PWD FROM USERINFO WHERE ID = #{id} AND PWD = #{pwd}
	</select>




</mapper>